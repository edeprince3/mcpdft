
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import bpsdp
symmetry c1
molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5
}

set {
  basis sto-3g
}

set bpsdp {
  print 1
}

energy('bpsdp')

bpsdp.exampleFN()

